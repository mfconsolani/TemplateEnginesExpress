</ul>

{{#if data}}
<table>
    <tr>
        <th>Nombre</th>
        <th>Precio</th>
        <th>Foto</th>
    </tr>
    {{#each data}}
    <tr>
        <td>{{this.title}}</td>
        <td>${{this.price}}</td>
        <td>{{this.thumbnail}}</td>
    </tr>
    {{/each}}
    </table>
    
    {{else}}
    <p>No se encontraron productos</p>
    {{/if}}